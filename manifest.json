{
  "manifest_version": 2,
  "name": "Quack",
  "version": "1.0",
  "description": "Chrome extension to automatically solve Quizlet games.",
  "author": "Oliver Ni",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://quizlet.com/*/*-flash-cards/"],
      "js": ["set.js"]
    },
    {
      "matches": ["*://quizlet.com/*/micromatch"],
      "js": ["micromatch.js"]
    }
  ]
}
